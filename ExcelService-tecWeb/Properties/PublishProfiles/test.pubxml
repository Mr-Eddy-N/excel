            <Scope>web.config$</Scope>
            <Match>/configuration/appSettings/add[@key='ClientId']/@value</Match>
            <Description>Value for ClientId here.</Description>
            <DefaultValue>762c093a-9b80-4661-96b2-8927bdf21a6c</DefaultValue>
            <Value>762c093a-9b80-4661-96b2-8927bdf21a6c</Value>
            <Tags>applicationSettings</Tags>
        </MsDeployDeclareParameters><MsDeployDeclareParameters Include="ClientSecret">
            <Kind>XmlFile</Kind>
            <Scope>web.config$</Scope>
            <Match>/configuration/appSettings/add[@key='ClientSecret']/@value</Match>
            <Description>Value for ClientSecret here.</Description>
            <DefaultValue>j91oVnkAKlVwNukqimMlgCJYmRkcCbfYmq6wZHYXODg=</DefaultValue>
            <Value>j91oVnkAKlVwNukqimMlgCJYmRkcCbfYmq6wZHYXODg=</Value>
            <Tags>applicationSettings</Tags>
        </MsDeployDeclareParameters></ItemGroup>
</Project>